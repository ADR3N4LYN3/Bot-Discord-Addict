# Token du bot Discord (récupéré depuis Discord Developer Portal)
DISCORD_TOKEN=VOTRE_TOKEN_BOT_ICI

# ID du bot (OBLIGATOIRE pour les commandes)
# Récupéré depuis Discord Developer Portal > Application > Application ID
CLIENT_ID=VOTRE_CLIENT_ID_ICI

# ID du serveur Discord (OBLIGATOIRE pour les commandes)
# Clic droit sur ton serveur > Copier l'identifiant du serveur
GUILD_ID=VOTRE_GUILD_ID_ICI

# ID du rôle à attribuer (0 = désactivé)
VERIFIED_ROLE_ID=0

# ID du channel Discord pour les logs de validation
LOG_CHANNEL_ID=0
